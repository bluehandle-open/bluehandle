<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.whyun.bluetooth" android:versionCode="6"
	android:versionName="1.4.1">
	<uses-sdk android:minSdkVersion="7"></uses-sdk>
	<uses-permission android:name="android.permission.VIBRATE"></uses-permission>
	<uses-permission android:name="android.permission.BLUETOOTH" />
	<uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
	<uses-permission android:name="android.permission.INTERNET" />

	<uses-permission android:name="android.permission.READ_PHONE_STATE">
	</uses-permission>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE">
	</uses-permission>
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION">
	</uses-permission>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE">
	</uses-permission>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE">
	</uses-permission>
	
	<uses-permission android:name="android.permission.WAKE_LOCK"/>
	
	<application android:icon="@drawable/icon"
		android:label="@string/app_name">
		<meta-data android:name="YOUMI_CHANNEL" android:value="10020"></meta-data><!-- 机锋网 -->
		
		<activity android:name="com.whyun.activity.MainActivity"
			android:screenOrientation="portrait"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category
					android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="com.whyun.activity.HandleWebActivity.HandleWebActivity"
			android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
			android:screenOrientation="landscape"
			android:label="@string/app_name">
		</activity>
		<activity android:name="com.whyun.activity.HandleNativeActivity"			
			android:screenOrientation="landscape"
			android:label="@string/app_name">
		</activity>
		<service android:name="com.whyun.service.DownloadService"/>  
		
		<activity android:name="net.youmi.android.AdYMActivity"
		android:configChanges="keyboard|keyboardHidden|orientation"/>
		<activity android:name="net.youmi.android.AdActivity"
		android:configChanges="keyboard|keyboardHidden|orientation"/>	
		<activity android:configChanges="keyboard|keyboardHidden|orientation"
		android:name="net.youmi.android.appoffers.YoumiOffersActivity" >
		</activity>
		
		<activity android:name="com.whyun.activity.ConfigActivity" ></activity>	
		
		<activity
            android:label="开心网" android:icon="@drawable/kaixin"
            android:name="com.whyun.activity.share.KaiXinShareActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <data android:mimeType="text/plain" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:label="人人网" android:icon="@drawable/renren"
            android:name="com.whyun.activity.share.RenRenShareActivity">
            <intent-filter>  
                <action android:name="android.intent.action.SEND" />
                <data android:mimeType="text/plain" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:label="新浪微博" android:icon="@drawable/xinlang"
            android:name="com.whyun.activity.share.XinLangWeiBoShareActivity">
            <intent-filter>  
                <action android:name="android.intent.action.SEND" />
                <data android:mimeType="text/plain" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:label="腾讯微博" android:icon="@drawable/qqweibo"
            android:name="com.whyun.activity.share.TengXunWeiBoShareActivity">
            <intent-filter>  
                <action android:name="android.intent.action.SEND" />
                <data android:mimeType="text/plain" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:label="网易微博" android:icon="@drawable/wangyiweibo"
            android:name="com.whyun.activity.share.WangYiWeiBoShareActivity">
            <intent-filter>  
                <action android:name="android.intent.action.SEND" />
                <data android:mimeType="text/plain" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.whyun.activity.KeyListActivity"
            android:screenOrientation="portrait">           
        </activity>
        <activity
            android:name="com.whyun.activity.KeyAddActivity"
            android:screenOrientation="portrait">            
        </activity>
        <activity android:name="com.whyun.activity.GuideActivity"
               	  android:theme="@android:style/Theme.NoTitleBar"
               	  android:configChanges="orientation|keyboardHidden">
        </activity>
        <receiver android:name="net.youmi.android.YoumiReceiver">
		    <intent-filter>
		        <action android:name="android.intent.action.PACKAGE_ADDED"/>
		        <action android:name="android.intent.action.PACKAGE_INSTALL"/>
		        <data android:scheme="package"/>
		    </intent-filter>
		</receiver>
	</application>	
</manifest>
